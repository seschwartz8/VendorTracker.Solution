@using VenderTracker.Models
@{
  Layout = "_Layout";
}
  <h1>Venders Index</h1>

    @if (Model.Count == 0)
    {
      <h4>You have no venders!</h4>
    }

  <ul>
    @foreach (Vender vender in Model)
    {
      <a href=""><li>@vender.Name</li></a>
    }
  </ul>

  <a href="/venders/new">Add New Vender</a>
  <a href="/venders/deleteAll">Delete All Venders</a>

